{
	"name": "Grubby Theme",
	"colors": {
		"editor.background": "#1d2021",
        "editor.foreground": "#ebdbb2",
        "editor.selectionBackground": "#07667859",
		"editorWhitespace.foreground": "#504945",
        "editor.lineHighlightBackground": "#282828",
        "editorLineNumber.foreground": "#665c54",
        "editorLineNumber.activeForeground": "#a89984",
        "editorBracketMatch.background": "#504945",
        "editorBracketMatch.border": "#7c6f64",
        "editorBracketHighlight.unexpectedBracket.foreground": "#D1243B",
        "editor.wordHighlightBackground": "#32593dB3",
        "editor.wordHighlightStrongBackground": "#532b2eB3",
        "editorIndentGuide.background1": "#50494566",
        "editorIndentGuide.activeBackground1": "#504945",
        "editorRuler.foreground": "#3c3836",
        "editor.findMatchBackground": "#532b2e",
        "editor.findMatchHighlightBackground": "#d79a2140",

        // terminal colors
        "terminal.ansiBlack": "#080808",
        "terminal.ansiBlue": "#83a598",
        "terminal.ansiBrightBlack": "#080808",
        "terminal.ansiBrightBlue": "#83a598",
        "terminal.ansiBrightCyan": "#81AE70",
        "terminal.ansiBrightGreen": "#98971A",
        "terminal.ansiBrightMagenta": "#d3869b",
        "terminal.ansiBrightRed": "#fb4934",
        "terminal.ansiBrightWhite": "#f9f5d7",
        "terminal.ansiBrightYellow": "#D79921",
        "terminal.ansiCyan": "#81AE70",
        "terminal.ansiGreen": "#98971A",
        "terminal.ansiMagenta": "#d3869b",
        "terminal.ansiRed": "#EA4430",
        "terminal.ansiWhite": "#f9f5d7",
        "terminal.ansiYellow": "#D79921",
        "terminal.background": "#1d2021",
        "terminal.foreground": "#ebdbb2",
	},
	"tokenColors": [
        {
            "name": "keyword or other syntax",
            "scope": "punctuation.terminator,constant.character.escape,punctuation.quasi.element",
            "settings": {
                "foreground": "#CC7832"
            }
        },
        {
            "name": "no highlight",
            "scope": "variable.parameter,variable.other,string.unquoted.argument.shell,storage.type.object,storage.type.java,constant.other.option",
            "settings": {
                "foreground": "#ebdbb2"
            }
        },
		{
			"scope": "entity.name.function,support.function,support.type.property-name.json,keyword.control.twig",
			"settings": {
				"foreground": "#83a598",
			}
		},
        {
            "scope": [
                "meta.embedded",
                "source.groovy.embedded",
                "string meta.image.inline.markdown",
                "variable.legacy.builtin.python"
            ],
            "settings": {
                "foreground": "#ebdbb2"
            }
        },
        {
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#928374"
            }
        },
        {
            "name": "block comment",
            "scope": "comment.block",
            "settings": {
                "foreground": "#79740e",
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#CC7832"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "variable.other.enummember",
                "keyword.operator.plus.exponent",
                "keyword.operator.minus.exponent"
            ],
            "settings": {
                "foreground": "#8ec07c"
            }
        },
        {
            "scope": "constant.regexp",
            "settings": {
                "foreground": "#b16286"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#CC7832"
            }
        },
        {
            "scope": [
                "entity.name.tag.css",
                "entity.name.tag.less"
            ],
            "settings": {
                "foreground": "#CC7832"
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#d79921"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.class.css",
                "source.css entity.other.attribute-name.class",
                "entity.other.attribute-name.id.css",
                "entity.other.attribute-name.parent-selector.css",
                "entity.other.attribute-name.parent.less",
                "source.css entity.other.attribute-name.pseudo-class",
                "entity.other.attribute-name.pseudo-element.css",
                "source.css.less entity.other.attribute-name.id",
                "entity.other.attribute-name.scss"
            ],
            "settings": {
                "foreground": "#CC7832"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#db4934"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#CC7832"
            }
        },
        {
            "scope": "markup.heading",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#CC7832"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.strikethrough",
            "settings": {
                "fontStyle": "strikethrough"
            }
        },
        {
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#8ec07c"
            }
        },
        {
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#b8bb26"
            }
        },
        {
            "scope": "markup.changed",
            "settings": {
                "foreground": "#CC7832"
            }
        },
        {
            "scope": "punctuation.definition.quote.begin.markdown",
            "settings": {
                "foreground": "#79740e"
            }
        },
        {
            "scope": "punctuation.definition.list.begin.markdown",
            "settings": {
                "foreground": "#83a598"
            }
        },
        {
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#b8bb26"
            }
        },
        {
            "name": "brackets of XML/HTML tags",
            "scope": "punctuation.definition.tag",
            "settings": {
                "foreground": "#a89984"
            }
        },
        {
            "scope": [
                "meta.preprocessor",
                "entity.name.function.preprocessor"
            ],
            "settings": {
                "foreground": "#CC7832"
            }
        },
        {
            "scope": "meta.preprocessor.string",
            "settings": {
                "foreground": "#b8bb26"
            }
        },
        {
            "scope": "meta.preprocessor.numeric",
            "settings": {
                "foreground": "#8ec07c"
            }
        },
        {
            "scope": "meta.diff.header",
            "settings": {
                "foreground": "#CC7832"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "foreground": "#CC7832"
            }
        },
        {
            "scope": "storage.type",
            "settings": {
                "foreground": "#CC7832"
            }
        },
        {
            "scope": [
                "storage.modifier",
                "keyword.operator.noexcept"
            ],
            "settings": {
                "foreground": "#CC7832"
            }
        },
        {
            "scope": [
                "string",
                "meta.embedded.assembly",
            ],
            "settings": {
                "foreground": "#b8bb26"
            }
        },
        {
            "scope": "string.tag",
            "settings": {
                "foreground": "#b8bb26"
            }
        },
        {
            "scope": "string.value",
            "settings": {
                "foreground": "#b8bb26"
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#b8bb26"
            }
        },
        {
            "name": "String interpolation",
            "scope": [
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end",
                "punctuation.section.embedded"
            ],
            "settings": {
                "foreground": "#CC7832"
            }
        },
        {
            "name": "Reset JavaScript string interpolation expression",
            "scope": [
                "meta.template.expression"
            ],
            "settings": {
                "foreground": "#ebdbb2"
            }
        },
        {
            "scope": [
                "support.type.vendored.property-name",
                "support.type.property-name",
                "source.css variable",
                "source.coffee.embedded"
            ],
            "settings": {
                "foreground": "#ebdbb2"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#CC7832"
            }
        },
        {
            "scope": "keyword.control",
            "settings": {
                "foreground": "#CC7832"
            }
        },
        {
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#ebdbb2"
            }
        },
        {
            "scope": [
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.cast",
                "keyword.operator.sizeof",
                "keyword.operator.alignof",
                "keyword.operator.typeid",
                "keyword.operator.alignas",
                "keyword.operator.instanceof",
                "keyword.operator.logical.python",
                "keyword.operator.wordlike"
            ],
            "settings": {
                "foreground": "#CC7832"
            }
        },
        {
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#8ec07c"
            }
        },
        {
            "scope": [
                "punctuation.section.embedded.begin.php",
                "punctuation.section.embedded.end.php"
            ],
            "settings": {
                "foreground": "#CC7832"
            }
        },
        {
            "scope": "constant.sha.git-rebase",
            "settings": {
                "foreground": "#8ec07c"
            }
        },
        {
            "name": "coloring of the Java import and package identifiers",
            "scope": [
                "storage.modifier.import.java",
                "variable.language.wildcard.java",
                "storage.modifier.package.java"
            ],
            "settings": {
                "foreground": "#ebdbb2"
            }
        },
        {
            "name": "this.self",
            "scope": "variable.language",
            "settings": {
                "foreground": "#CC7832"
            }
        },
        {
            "name": "built in properties",
            "scope": [
                "support.constant.property-value",
                "keyword.control.at-rule",
            ],
            "settings": {
                "foreground": "#d79921"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "newOperator": "#ebdbb2",
        "stringLiteral": "#b8bb26",
        "customLiteral": "#ebdbb2",
        "numberLiteral": "#8ec07c"
    }
}
